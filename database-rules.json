{
  "rules": {
    "messages": {
      ".write": "auth.token.admin === true",
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "users": {
      ".read": "auth.token.admin === true",
      "$uid": {
        ".write": "$uid === auth.uid"
      }
    }
  }
}